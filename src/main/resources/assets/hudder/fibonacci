;mute;
{fibonacci=""}
{N=0}
{num1=0}{num2=1}
{expectedwidth=0}
#while N<93
	{N=N+1}
	{numstr=str(num1,true)}
	{expectedwidth=expectedwidth+(strwidth(concat(numstr," ")))}
	#if expectedwidth>=width/2
		{numstr=concat("\n", numstr)}
		{expectedwidth=0}
	{fibonacci=concat(fibonacci, concat(numstr," "))}
	{num3=num2+num1}
	{num1=num2}
	{num2=num3}
;topleft,1;
{fibonacci}
